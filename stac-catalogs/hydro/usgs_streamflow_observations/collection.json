{
    "id": "usgs_streamflow_observations",
    "stac_version": "1.0.0-beta.2",
    "description": "Streamflow observations for CONUS-wide USGS stations",
    "links": [
        {
            "rel": "root",
            "href": "..\\catalog.json",
            "type": "application/json"
        },
        {
            "rel": "license",
            "href": "https://http://example.com",
            "type": "text/html",
            "title": "Replace this link and title with your own license"
        },
        {
            "rel": "parent",
            "href": "..\\catalog.json",
            "type": "application/json"
        }
    ],
    "stac_extensions": [
        "collection-assets"
    ],
    "assets": {
        "zmetadata": {
            "href": "https://pangeo-usgs-flow-observations.s3.amazonaws.com/.zmetadata",
            "description": "Consolidated metadata file for Zarr store",
            "type": "application/json",
            "roles": [
                "metadata",
                "zarr-consolidated-metadata"
            ]
        }
    },
    "extent": {
        "spatial": {
            "bbox": [
                [
                    -180,
                    -90,
                    180,
                    90
                ]
            ]
        },
        "temporal": {
            "interval": [
                [
                    "0001-01-01T00:00:00Z",
                    "9999-12-31T23:59:59.999999Z"
                ]
            ]
        }
    },
    "license": "proprietary",
    "providers": [
        {
            "name": "Add in links/titles for providers of any of the following roles",
            "roles": [
                "producer",
                "licensor",
                "provessor",
                "host"
            ],
            "url": "https://example.com"
        }
    ]
}